<?xml version="1.0" encoding="UTF-8"?>
<workflow:Workflow
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:workflow="http://www.example.org/workflow"
    xsi:schemaLocation="http://www.example.org/workflow metamodel/workflow.ecore">
  <recipes name="R1">
    <commands
        description="creaza director">
      <programs
          name_exec="mkdir">
        <parameters
            data="NEWDIR"/>
      </programs>
    </commands>
    <commands
        xsi:type="workflow:IfThenElseCommand"
        description="### generate T1.nii.gz from DICOM">
      <condition
          expression="! -f $PRD/data/T1/T1.nii.gz"
          true_branch="//@recipes.0/@commands.2/@programs.0"/>
    </commands>
    <commands
        description="ggggg">
      <programs
          name_exec="echo ">
        <parameters
            data="&quot;generating T1.nii.gz from DICOM&quot;"/>
      </programs>
    </commands>
  </recipes>
</workflow:Workflow>
